@Article{Hlatky1989,
  Title                    = {A brief self-administered questionnaire to determine functional capacity (The Duke Activity Status Index)},
  Author                   = {Mark A. Hlatky and Robin E. Boineau and Michael B. Higginbotham and Kerry L. Lee and Daniel B. Mark and Robert M. Califf and Frederick R. Cobb and David B. Pryor},
  Journal                  = {The American Journal of Cardiology},
  Year                     = {1989},
  Number                   = {10},
  Pages                    = {651 - 654},
  Volume                   = {64},

  Abstract                 = {To develop a brief, self-administered questionnaire that accurately measures functional capacity and assesses aspects of quality of life, 50 subjects undergoing exercise testing with measurement of peak oxygen uptake were studied. All subjects were questioned about their ability to perform a variety of common activities by an interviewer blinded to exercise test findings. A 12-item scale (the Duke Activity Status index) was then developed that correlated well with peak oxygen uptake (Spearman correlation coefficient 0.80). To test this new index, an independent group of 50 subjects completed a self-administered questionnaire to determine functional capacity and underwent exercise testing with measurement of peak oxygen uptake. The Duke Activity Status Index correlated significantly (p &lt; 0.0001) with peak oxygen uptake (Spearman correlation coefficient 0.58) in this independent sample. The Duke Activity Status Index is a valid measure of functional capacity that can be obtained by self-administered questionnaire.},
  Doi                      = {10.1016/0002-9149(89)90496-7},
  ISSN                     = {0002-9149},
  Owner                    = {u_bviall},
  Timestamp                = {2012.09.17},
  Url                      = {http://www.sciencedirect.com/science/article/pii/0002914989904967}
}

